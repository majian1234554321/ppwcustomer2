<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BottomBehavior">
        <attr name="anchor_id" format="integer"/>
        <attr name="bottom_padding" format="float"/>
    </declare-styleable>

    <declare-styleable name="TitleBarView">
        <attr name="textColor" format="color"/>
        <attr name="textValue" format="string"/>
        <attr name="textSize" format="dimension"/>
        <attr name="imageSrc" format="reference"/>
        <attr name="titleBarBackground" format="color"/>
    </declare-styleable>


    <declare-styleable name="ItemEntryView">
        <attr name="ievtextColor" format="color"/>
        <attr name="ievtextValue" format="string"/>
        <attr name="ievtextSize" format="dimension"/>
        <attr name="ievimageSrc" format="reference"/>
        <attr name="ievbackground" format="color"/>
    </declare-styleable>

</resources>