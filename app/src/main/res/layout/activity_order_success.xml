<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.activity.takeout.OrderSuccessActivity">
    <com.yjhh.common.view.TitleBarView android:layout_width="match_parent"
                                       app:textColor="@color/white"
                                       android:id="@+id/tbv_title"
                                       app:textValue="下单成功"
                                       android:layout_height="wrap_content"/>


    <RelativeLayout android:layout_width="match_parent"
                    android:layout_below="@+id/tbv_title"
                    android:layout_height="wrap_content">


        <TextView android:layout_width="match_parent" android:layout_height="50dp"
                  android:gravity="center"
                  android:id="@+id/tv_check"
                  android:text="查看订单"
        />

    </RelativeLayout>


</RelativeLayout>