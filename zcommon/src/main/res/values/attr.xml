<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BottomBehavior">
        <attr name="anchor_id" format="integer"/>
        <attr name="bottom_padding" format="float"/>
    </declare-styleable>

    <declare-styleable name="RatingBar">
        <!--尺寸值-->
        <attr name="starImageSize" format="dimension" />
        <!--星星间距-->
        <attr name="starPadding" format="dimension" />
        <!--星星总数-->
        <attr name="allStarsCount" format="integer" />
        <!--空白的星星资源文件值-->
        <attr name="starEmpty" format="reference" />
        <!--满星资源文件值-->
        <attr name="starFill" format="reference" />
        <!--半星资源文件值-->
        <attr name="starHalf" format="reference" />
        <!--是否可点击boolean值-->
        <attr name="clickable" format="boolean" />
        <!--当前进度float值-->
        <attr name="curSelectValue" format="float" />
        <!--每次进度方式的值，整星还是半星-->
        <attr name="stepSize">
            <enum name="Half" value="0" />
            <enum name="Full" value="1" />
        </attr>





        <!--未选中-->
        <attr name="starEmptyRes" format="reference" />
        <!--选中半个-->
        <attr name="starHalfRes" format="reference" />
        <!--选中全部-->
        <attr name="starSelectedRes" format="reference" />
        <!--星星的总数-->
        <attr name="startTotalNumber" format="integer" />
        <!--默认选中的数量 -->
        <attr name="selectedNumber" format="float" />
        <!--星星的间距-->
        <attr name="starDistance" format="dimension" />
        <!--星星的宽度-->
        <attr name="starWidth" format="dimension" />
        <!--星星的高度-->
        <attr name="starHeight" format="dimension" />
        <!--是不是只画整个星星，不画半个-->
        <attr name="starIsFull" format="boolean" />








    </declare-styleable>


    <declare-styleable name="WaveSideBarView">
        <attr name="sidebarTextColor" format="color|reference"/>
        <attr name="sidebarBackgroundColor" format="color|reference"/>
        <attr name="sidebarChooseTextColor" format="color|reference"/>
        <attr name="sidebarTextSize" format="dimension"/>
        <attr name="sidebarLargeTextSize" format="dimension"/>
        <attr name="sidebarRadius" format="dimension"/>
        <attr name="sidebarBallRadius" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="lib_ui_view_SideBar">
        <attr name="lib_ui_view_sidebar_type">
            <enum name="normal" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="lib_ui_view_sidebar_letters" format="string" />
        <attr name="lib_ui_view_sidebar_maxCount" format="integer" />
    </declare-styleable>


    <declare-styleable name="EnhanceTabLayout">
        <attr name="tab_Mode" format="enum">
            <enum name="mode_fixed" value="1"/>
            <enum name="mode_scrollable" value="2"/>
        </attr>
        <attr name="tabIndicatorColor" format="color"/>
        <attr name="tabSelectTextColor" format="color"/>
        <attr name="tabTextColor" format="color"/>
        <attr name="tabIndicatorHeight" format="dimension"/>
        <attr name="tabIndicatorWidth" format="dimension"/>
        <attr name="tabTextSize" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="ItemEntryView">
        <attr name="ievtextColor" format="color"/>
        <attr name="ievtextValue" format="string"/>
        <attr name="ievtextContentValue" format="string"/>
        <attr name="ievtextSize" format="dimension"/>

        <attr name="ievtextpadding" format="dimension"/>

        <attr name="ievimageSrc" format="reference"/>
        <attr name="ievbackground" format="color"/>
        <attr name="ievunderline" format="boolean"/>
        <attr name="ievdisplaytextContent" format="boolean"/>
        <attr name="ievarrow" format="boolean"/>

        <attr name="ievdrwableleft" format="reference"/>
    </declare-styleable>



    <declare-styleable name="ItemEntryView2">
        <attr name="ievtextColor2" format="color"/>
        <attr name="ievtextValue2" format="string"/>
        <attr name="ievtexthint" format="string"/>
        <attr name="ievrighttextValue" format="string"/>
        <attr name="ievRightTextColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="lib_pub_ToggleButton">
        <attr name="lib_pub_tbtn_colorThumb" format="color" />
        <attr name="lib_pub_tbtn_colorTrackOpen" format="color" />
        <attr name="lib_pub_tbtn_colorTrackOff" format="color" />
        <attr name="lib_pub_tbtn_colorPadding" format="color" />
        <attr name="lib_pub_tbtn_padding" format="dimension" />
        <attr name="lib_pub_tbtn_duration" format="integer" />
    </declare-styleable>

    <!-- Public dialog -->
    <style name="lib_pub_dialog_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <declare-styleable name="MeiTuanFoodView">
        <attr name="leftSum" format="integer"/>
        <attr name="rightSum" format="integer"/>
        <attr name="topItemHeight" format="dimension"/>
    </declare-styleable>



    <declare-styleable name="NineGridView">
        <attr name="ngv_singleImageSize" format="dimension"/>
        <attr name="ngv_singleImageRatio" format="float"/>
        <attr name="ngv_gridSpacing" format="dimension"/>
        <attr name="ngv_maxSize" format="integer"/>
        <attr name="ngv_mode" format="enum">
            <enum name="fill" value="0"/>
            <enum name="grid" value="1"/>
        </attr>

    </declare-styleable>




    <declare-styleable name="SuperCircleView">
        <attr name="insideCircleRadius" format="dimension"/>
        <attr name="insideCircleColor" format="color"/>
        <attr name="insideStrokeWidth" format="dimension"/>


        <attr name="outsideCircleRadius" format="dimension"/>
        <attr name="outsideCircleColor" format="color"/>
        <attr name="outsideStrokeWidth" format="dimension"/>

        <attr name="CircleLTextSize" format="dimension"/>

        <attr name="CircleSTextSize" format="dimension"/>


    </declare-styleable>


    <declare-styleable name="TitleBarView">
        <attr name="textColor" format="color"/>
        <attr name="textValue" format="string"/>
        <attr name="textRightValue" format="string"/>
        <attr name="textRightDisplay" format="boolean"/>
        <attr name="textSize" format="dimension"/>
        <attr name="imageSrc" format="reference"/>
        <attr name="imageDis" format="boolean"/>
        <attr name="titleBarBackground" format="color"/>
    </declare-styleable>

    <style name="MyCheckBox" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">#888888</item>
        <item name="colorControlActivated">#FF552E</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
    </style>





</resources>